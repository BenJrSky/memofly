=\"400\" v-bind:options=\"chartOptions\" v-bind:series=\"series\"/>\n              </div>\n               <div v-if=\"series.length==0\" class=\"justify-content-center pt-3\">\n                <Badge class=\"w-50\" message=\"Non hai appuntamenti\"></Badge>\n              </div>\n          </div>\n      </div>\n</template>\n\n<script lang=\"ts\">\n\n\nimport { Component, Prop, Vue } from 'vue-property-decorator';\nimport VueApexCharts from \"vue-apexcharts\";\nimport Badge from './../common/Badge.vue';\n\nimport { GETAppointmentsPerCalendar } from './../../memoFly-api/dashboard/GETAppointmentsPerCalendar'\n\n@Component({\n  components: {\n    VueApexCharts,\n    Badge,\n  },\n})\n\n\nexport default class AppuntamentiCalendarioChart extends Vue {\n  \n    public series:any = [];\n\n\n    data() {\n\n\n      this.series = [];\n\n      this.init();  \n\n    return {\n      series: this.series,\n      chartOptions: {\n        chart: {\n          type: 'line',\n          toolbar: {\n            show: false\n          }\n        },\n        stroke: {\n          width: 3,\n          curve: 'smooth',\n          dashArray: [0, 0, 0]\n        },\n        xaxis: {\n          categories: ['Gen','Feb','Mar','Apr','Mag','Giu','Lug','Ago','Set','Ott','Nov','Dic'],\n        },\n        fill: {\n          type: 'gradient',\n          gradient: {\n            shade: 'dark',\n            gradientToColors: [ '#ED4F5A','#3BD9B6','#0000EE'],\n            shadeIntensity: 1,\n            type: 'horizontal',\n            opacityFrom: 1,\n            opacityTo: 1,\n            stops: [0, 100, 100, 100]\n          },\n        },\n        markers: {\n          size: 4,\n          colors: [\"#FFA41B\"],\n          strokeColors: \"#fff\",\n          strokeWidth: 2,\n          hover: {\n            size: 7,\n          }\n        },\n        yaxis: {\n          min: 0,\n          title: {\n            text: '',\n          },\n        }\n      }\n      \n    }\n\n    }\n\n    init(){\n\n        GETAppointmentsPerCalendar()\n            .then((dataAppointments:any)=>{\n               if(dataAppointments.data){\n                  return this.series = dataAppointments.data.map((calendar:any) =>{\n                      return {\n                        name:calendar.nome,\n                        data:calendar.data,\n                      }\n                  });\n               }\n            });\n    }\n\n}\n\n\n\n\n</script>\n\n<style lang=\"scss\">\n\n.fc .fc-toolbar-title{\n  font-size: 1.5rem;\n}\n\n</style>\n"]}]}